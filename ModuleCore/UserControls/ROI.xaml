<UserControl
    x:Class="ModuleCore.UserControls.ROI"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ModuleCore.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="100"
    d:DesignWidth="100"
    mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/DictionaryCore.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Canvas>
        <ContentControl
            Name="roi"
            Canvas.Left="0"
            Canvas.Top="0"
            Width="100"
            Height="100"
            Style="{StaticResource OnlySizeStyle}">
            <ContentControl.ContextMenu>
                <ContextMenu>
                    <MenuItem Click="RemoveMe" Header="删除" />
                </ContextMenu>
            </ContentControl.ContextMenu>
        </ContentControl>
    </Canvas>

</UserControl>
